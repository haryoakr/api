                    </ul>
                </div>
            </div>
        </nav>
        
        
        <br>
        <!-- Contact-->
        <section class="page-section" id="faq">
            <div class="container">
                <div class="text-center">
                    <h2 class="section-heading text-uppercase">QUIZ LIST</h2>
                </div>
                <% var i = 1; while(i<=data_vocabularies){ %>
                    <li>
                          <a href="/quiz/start?min=<%= i %>&max=<%= i+39 %>&lang=eng"><%= i %> - <%= i+39 %> (ENG)</a>
                          &nbsp;&nbsp;&nbsp;&nbsp;
                          &nbsp;&nbsp;&nbsp;&nbsp;
                          &nbsp;&nbsp;&nbsp;&nbsp;
                          <a href="/quiz/start?min=<%= i %>&max=<%= i+39 %>&lang=id"><%= i %> - <%= i+39 %> (ID)</a>
                                               
                    </li>
                <% i+=40; }%>
                
            </div>
        </section>
        
    
       
       
        
